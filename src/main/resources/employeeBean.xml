<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="emp1" class="bean.Employee">
        <property name="empId" value="1"/>
        <property name="empName" value="Nguyen Van A"/>
        <property name="address" value="Ha Noi"/>
    </bean>

    <bean id="emp2" class="bean.Employee">
        <property name="empId" value="2"/>
        <property name="empName" value="Nguyen Van B"/>
        <property name="address" value="HCM"/>
    </bean>

</beans>